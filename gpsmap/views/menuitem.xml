<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <menuitem name="GPS" id="menu_root" groups="fleet.fleet_group_user" web_icon="fleet,static/description/icon.png"/>
    
    <menuitem name="Geolocation" parent="menu_root" id="gpsmap_geolocation" sequence="2" groups="fleet.fleet_group_user"/>
        
    <record id="action_gpsmap_maponline" model="ir.actions.client">
        <field name="name">Map Online</field>
        <field name="tag">gpsmap.maponline</field>
    </record>
    <menuitem id="menu_gpsmap_maponline" name="Online Map" sequence="1" parent="gpsmap_geolocation" action="action_gpsmap_maponline" groups="fleet.fleet_group_user"/>

    <record id="action_gpsmap_street" model="ir.actions.client">
        <field name="name">Street Online</field>
        <field name="tag">gpsmap.streetonline</field>
    </record>
    <menuitem id="menu_gpsmap_streetonline" sequence="2" name="Online Street"  parent="gpsmap_geolocation" action="action_gpsmap_street" groups="fleet.fleet_group_user"/>    






    <record id='fleet_position_action' model='ir.actions.act_window'>
        <field name="name">Histoy Map</field>
        <field name="res_model">gpsmap.positions</field>
    </record>
    <menuitem action="fleet_position_action" parent="fleet.fleet_vehicles" id="fleet_position_menu" groups="gpsmap_group_user"/>

    <record id='fleet_speed_action' model='ir.actions.act_window'>
        <field name="name">Speed</field>
        <field name="res_model">gpsmap.speed</field>
    </record>
    <menuitem action="fleet_speed_action" parent="fleet.fleet_vehicles" id="fleet_speed_menu" groups="gpsmap_group_user"/>
    
    
    
    
    
    
    <record id='fleet_geofence_action' model='ir.actions.act_window'>
        <field name="name">Geofence</field>
        <field name="res_model">gpsmap.geofence</field>
    </record>
    <menuitem action="fleet_geofence_action" parent="fleet.fleet_vehicles" id="fleet_geofence_menu" groups="gpsmap_group_user"/>

    <!-- ############################### -->    

    <record id='fleet_geofence_device_action' model='ir.actions.act_window'>
        <field name="name">Alerts Geofence</field>
        <field name="res_model">gpsmap.geofence_device</field>
    </record>
    <menuitem action="fleet_geofence_device_action" parent="fleet.fleet_vehicles" id="fleet_geofence_device_menu" groups="gpsmap_group_user"/>
    
    
    
    
    
    
</odoo>
