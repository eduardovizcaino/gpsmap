<?xml version="1.0" encoding="UTF-8"?>
<odoo>




    <menuitem name="GPS" id="menu_root" groups="fleet.fleet_group_user" web_icon="fleet,static/description/icon.png"/>
    
    <menuitem name="Geolocation" parent="menu_root" id="gpsmap_geolocation" sequence="2" groups="fleet.fleet_group_user"/>
        
    <record id="action_gpsmap_maponline" model="ir.actions.client">
        <field name="name">Map Online</field>
        <field name="tag">gpsmap.maponline</field>
    </record>
    <menuitem id="menu_gpsmap_maponline"  sequence="1" name="Online Map" parent="gpsmap_geolocation" action="action_gpsmap_maponline" groups="fleet.fleet_group_user"/>

    <record id="action_gpsmap_street" model="ir.actions.client">
        <field name="name">Street Online</field>
        <field name="tag">gpsmap.streetonline</field>
    </record>
    <menuitem id="menu_gpsmap_streetonline" sequence="2" name="Online Street"  parent="gpsmap_geolocation" action="action_gpsmap_street" groups="fleet.fleet_group_user"/>    


    <menuitem name="Vehicles" parent="fleet.menu_root" id="fleet.fleet_vehicles" sequence="2" groups="fleet_group_user"/>    

    <record id='action_gpsmap_position' model='ir.actions.act_window'>
        <field name="name">Histoy Map</field>
        <field name="res_model">gpsmap.positions</field>
    </record>
    <menuitem action="action_gpsmap_position" parent="fleet.fleet_vehicles" id="gpsmap_position_menu" groups="fleet.fleet_group_manager"/>

    <record id='action_gpsmap_speed' model='ir.actions.act_window'>
        <field name="name">Speed</field>
        <field name="res_model">gpsmap.speed</field>
    </record>
    <menuitem action="action_gpsmap_speed" parent="fleet.fleet_vehicles" id="gpsmap_speed_menu" groups="fleet.fleet_group_manager"/>
    
    


    
    <record id='action_gpsmap_geofence' model='ir.actions.act_window'>
        <field name="name">Geofence</field>
        <field name="res_model">gpsmap.geofence</field>
    </record>
    <menuitem action="action_gpsmap_geofence" parent="fleet.fleet_configuration" id="gpsmap_geofence_menu" groups="fleet.fleet_group_manager"/>

    <!-- ############################### -->    

    <record id='action_gpsmap_geofence_device' model='ir.actions.act_window'>
        <field name="name">Alerts Geofence</field>
        <field name="res_model">gpsmap.geofence_device</field>
    </record>
    <menuitem action="action_gpsmap_geofence_device" parent="fleet.fleet_configuration" id="gpsmap_geofence_device_menu" groups="fleet.fleet_group_manager"/>
    
    
    
    
    
    
</odoo>
